---
import BaseLayout from './BaseLayout.astro';

const {frontmatter, url} = Astro.props;
const {title, description, ogImage} = frontmatter;
---

<BaseLayout url={url} title={title} description={description} ogImage={ogImage}>

  <main class="p-4 mt-4">
    <div class="prose">
      <!--required with tailwind typography plugin to render html-->
      <slot/>
    </div>
  </main>
</BaseLayout>
